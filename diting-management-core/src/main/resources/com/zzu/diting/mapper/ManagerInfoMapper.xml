<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zzu.diting.mappers.ManagerMapper">
    <select id="queryManagerInfoByName" resultType="com.zzu.diting.entity.ManagerInfo">
        select *
        from manager_info
        where name like "%"#{name}"%"

    </select>

</mapper>